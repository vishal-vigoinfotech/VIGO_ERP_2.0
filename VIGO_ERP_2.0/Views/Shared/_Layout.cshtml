        <div id="modal_emp_passreset" class="hidden fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50">
            <div class="bg-white rounded-2xl p-8 w-96 relative shadow-2xl">
                <h2 class="text-xl font-bold mb-4 flex items-center space-x-2">
                    <i class="fa-solid fa-lock text-indigo-600"></i>
                    <span>Change Password</span>
                </h2>

                <!-- Current Password -->
                <div class="mb-4">
                    <label class="block mb-1 font-medium text-gray-700">Current Password</label>
                    <input type="password" id="OldPass" class="w-full px-4 py-2 border rounded-lg focus:outline-none focus:ring-2 focus:ring-indigo-500">
                    <span id="lblerr_OldPass" class="text-red-500 text-sm mt-1 block"></span>
                </div>

                <!-- New Password -->
                <div class="mb-4">
                    <label class="block mb-1 font-medium text-gray-700">New Password</label>
                    <input type="password" id="NewPass" class="w-full px-4 py-2 border rounded-lg focus:outline-none focus:ring-2 focus:ring-indigo-500">
                    <span id="lblerr_NewPass" class="text-red-500 text-sm mt-1 block"></span>
                </div>

                <!-- Confirm Password -->
                <div class="mb-4">
                    <label class="block mb-1 font-medium text-gray-700">Confirm New Password</label>
                    <input type="password" id="ConfPass" class="w-full px-4 py-2 border rounded-lg focus:outline-none focus:ring-2 focus:ring-indigo-500">
                    <span id="lblerr_ConfPass" class="text-red-500 text-sm mt-1 block"></span>
                </div>

                <div class="flex justify-end space-x-2 mt-6">
                    <button onclick="$('#modal_emp_passreset').addClass('hidden')"
                            class="px-4 py-2 rounded-lg border border-gray-300 hover:bg-gray-100 transition">
                        Cancel
                    </button>
                    <button onclick="ResetPassword()"
                            class="px-4 py-2 rounded-lg bg-indigo-600 text-white hover:bg-indigo-700 transition">
                        Submit
                    </button>
                </div>
            </div>
        </div>






