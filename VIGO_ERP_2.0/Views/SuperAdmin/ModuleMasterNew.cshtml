
@{
    ViewBag.Title = "ModuleMasterNew";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}

<div class="row">
    <div class="col-sm-6 col-lg-6">
        <h6>Module Master</h6>
    </div>
    <div class="col-sm-6 col-lg-6 text-end">
        <h6 class="text-success">Manage Module Management</h6>
    </div>
    <div class="g.col-12 bg-white p-2 mt-3">
        <h6>Manage Module Master</h6>
        <div class="row">
            <div class="col-sm-6 col-md-6 col-lg-3 glass-container">
                <input type="text" id="txtModuleName" name="ModuleName" required onchange="validateModuleName()" />
                <label for="ModuleName">Module Name <span class="text-danger fw-bold"> *</span></label>
                <span id="lblerr_txtModuleName" class="text-danger"></span>
            </div>
            <div class="col-sm-6 col-md-6 col-lg-3 glass-container">
                <input type="text" id="txtModuleId" name="CompanyCode" required onchange="validateModuleId()" />
                <label for="ModuleId">Module Id<span class="text-danger fw-bold"> *</span></label>
                <span id="lblerr_txtModuleId" class="text-danger"></span>
            </div>
            <div class="col-sm-6 col-md-6 col-lg-4 glass-container">
                <input type="text" id="txtModuleDescriptiion" name="ModuleDescription" required onchange="validateModuleDescription()" />
                <label for="txtModuleDescriptiion">Module Description<span class="text-danger fw-bold"> *</span></label>
                <span id="lblerr_txtModuleDescriptiion" class="text-danger"></span>
            </div>
            <div class="col-sm-6 col-md-6 col-lg-2 text-lg-end">
                <button type="button" class="text-white cmp-btn no-hover-effect mt-3" id="btnCompanyModuleMaster" onclick="Creatmodulemaster(this); return false;">Submit</button>
            </div>
        </div>
    </div>
    <div class="col-sm-12 bg-white p-1 mt-3">
        <h6 class="mb-3">Module Master Details</h6>
        @*<button id="download-xlsx" class="mb-3 cmp_excel_btn">Excel</button>*@
        <div id="divCompanyModuleDetails"></div>
    </div>
</div>
<script>
    $(document).ready(function () {
        GetAllCompanyModuleDetails("VIEW");
    });
</script>

